    "from sklearn.metrics import calinski_harabasz_score, davies_bouldin_score\n",
    "from sklearn.cluster import KMeans\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "calinski_scores = []\n",
    "davies_scores = []\n",
    "k_values = list(range(2, 10))\n",
    "\n",
    "for k in k_values:\n",
    "    kmeans = KMeans(n_clusters=k, random_state=42)\n",
    "    labels = kmeans.fit_predict(df_scaled)\n",
    "    \n",
    "    calinski_scores.append(calinski_harabasz_score(df_scaled, labels))\n",
    "    davies_scores.append(davies_bouldin_score(df_scaled, labels))\n",
    "\n",
    "# Plotting both scores side-by-side\n",
    "fig, ax = plt.subplots(1, 2, figsize=(14, 5))\n",
    "\n",
    "# Calinski-Harabasz Plot\n",
    "ax[0].plot(k_values, calinski_scores, marker='o', color='blue')\n",
    "ax[0].set_title('Calinski-Harabasz Score vs Clusters')\n",
    "ax[0].set_xlabel('Number of Clusters (k)')\n",
    "ax[0].set_ylabel('CH Score')\n",
    "ax[0].grid(True)\n",
    "\n",
    "# Davies-Bouldin Plot\n",
    "ax[1].plot(k_values, davies_scores, marker='o', color='red')\n",
    "ax[1].set_title('Davies-Bouldin Score vs Clusters')\n",
    "ax[1].set_xlabel('Number of Clusters (k)')\n",
    "ax[1].set_ylabel('DB Score (lower is better)')\n",
    "ax[1].grid(True)\n",
    "\n",
    "plt.tight_layout()\n",
    "plt.show()\n"
   ]
