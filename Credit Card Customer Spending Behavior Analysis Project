    {
     "data": {
      "image/png": "",
      "text/plain": [
       "<Figure size 1000x600 with 2 Axes>"
      ]
     },
     "metadata": {},
     "output_type": "display_data"
    }
   ],
   "source": [
    "import seaborn as sns\n",
    "import matplotlib.pyplot as plt\n",
    "import pandas as pd\n",
    "\n",
    "\n",
    "features = [\n",
    "    'TOTAL_PURCHASES',\n",
    "    'RATIO_ONEOFF_TO_TOTAL',\n",
    "    'BALANCE_TO_LIMIT_RATIO',\n",
    "    'CASH_ADVANCE_RATIO',\n",
    "    'PRC_FULL_PAYMENT',\n",
    "    'PAYMENTS'\n",
    "]\n",
    "\n",
    "cluster_means = df_cleaned.groupby('Cluster')[features].mean()\n",
    "\n",
    "normalized_means = (cluster_means - cluster_means.min()) / (cluster_means.max() - cluster_means.min())\n",
    "\n",
    "#  Plot heatmap\n",
    "plt.figure(figsize=(10, 6))\n",
    "sns.heatmap(normalized_means, cmap=\"YlGnBu\", annot=True, fmt=\".2f\",\n",
    "            linewidths=0.5, cbar_kws={'label': 'Normalized Value'})\n",
    "\n",
    "plt.title(\"🔍 Cluster Profiles (Normalized Feature Means)\", fontsize=14)\n",
    "plt.xlabel(\"Feature\", fontsize=12)\n",
    "plt.ylabel(\"Cluster\", fontsize=12)\n",
    "plt.xticks(rotation=45)\n",
    "plt.tight_layout()\n",
    "plt.show()\n",
    "\n"
   ]
  }
